function revarr(arr, start, end){
    while(start < end)
    {
        var temp = arr[start];
        arr[start]=arr[end];
        arr[end]=temp;
        start++;
        end--;
    }
}

function printarr(arr, size){
    for(var i=0; i<size; i++){
        console.log(arr[i]);
    }
}

var arr= [1,2,3,4,5];
var n=5;
printarr(arr,n);
revarr(arr,0,n-1);
console.log("Reversed array is");
printarr(arr,n);
